#Область ОбработчикиСобытий
Процедура ОбработкаПроведения(Отказ, Режим)

	// регистр РазряднаяСетка
	Движения.РазряднаяСетка.Записывать = Истина;
	Для Каждого ТекСтрокаНазначениеОклада Из НазначениеОклада Цикл
		Движение = Движения.РазряднаяСетка.Добавить();
		Движение.Период = Дата;
		Движение.Разряд = ТекСтрокаНазначениеОклада.Разряд;
		Движение.Ресторан = Ресторан;
		Движение.Оклад = ТекСтрокаНазначениеОклада.Оклад;
	КонецЦикла;

КонецПроцедуры
#КонецОбласти
